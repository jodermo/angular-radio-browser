{
  "name": "angular-radio-browser",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --crossOrigin=\"anonymus\"",
    "build:cordova": "ng build --outputPath=\"cordova-app/www/\" && npm run ng:cordova",
    "build:cordova:device": "ng build --outputPath=\"cordova-app/www/\" && npm run ng:cordova:device",
    "build:cordova:device:log": "ng build --outputPath=\"cordova-app/www/\" && npm run ng:cordova:device:log",
    "build:cordova:release": "ng build --outputPath=\"cordova-app/www/\" && npm run ng:cordova:release",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng:cordova": "cd cordova-app && npm run ng:build",
    "ng:cordova:device": "cd cordova-app && npm run ng:build:device",
    "ng:cordova:device:log": "cd cordova-app && npm run ng:build:device:log",
    "ng:cordova:release": "cd cordova-app && npm run ng:build:release",
    "cordova": "cd cordova-app && npm run build",
    "cordova:device": "cd cordova-app && npm run build:device",
    "cordova:device:log": "cd cordova-app && npm run build:device:log",
    "cordova:release": "cd cordova-app && npm run build:release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@ionic-native/core": "^5.23.0",
    "@ionic-native/media": "^5.23.0",
    "@ionic-native/music-controls": "^5.23.0",
    "@ionic/angular-toolkit": "^2.2.0",
    "cordova-plugin-music-controls2": "^3.0.3",
    "device-detector-js": "^2.2.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
